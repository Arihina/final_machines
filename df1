// DFA
digraph {
	rankdir=LR
	q4 [shape=doublecircle]
	q2 [shape=doublecircle]
	q1 [shape=circle]
	q3 [shape=circle]
	q0 [shape=circle]
	"" [shape=point]
	"" -> q0
	q0 -> q1 [label=a]
	q0 -> q0 [label=b]
	q0 -> q0 [label=c color=black]
	q1 -> q2 [label=a]
	q1 -> q1 [label=b]
	q1 -> q0 [label=c]
	q2 -> q3 [label=a]
	q2 -> q2 [label=b]
	q2 -> q0 [label=c]
	q3 -> q4 [label=a]
	q3 -> q3 [label=b]
	q3 -> q2 [label=c]
	q4 -> q0 [label=a]
	q4 -> q4 [label=b]
	q4 -> q3 [label=c]
}
